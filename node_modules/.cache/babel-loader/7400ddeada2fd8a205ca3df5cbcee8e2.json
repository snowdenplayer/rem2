p class=\"slide__text\">{{ person.text }}</p>\n        </div>\n      </splide-slide>\n    </splide>\n  </div>\n</template>\n\n<script>\nimport {Splide, SplideSlide} from '@splidejs/vue-splide';\nimport '@splidejs/splide/dist/css/themes/splide-sea-green.min.css';\nimport {feedbacks} from \"@/data/data.feedback\"\n\nexport default {\n  name: \"slider\",\n  data() {\n    return {\n      options: {\n        type: 'loop',\n        gap: '1rem',\n        perPage: 3,\n        perMove: 1,\n        focus  : 'center',\n        breakpoints: {\n          991: {\n            perPage: 2,\n          },\n          767: {\n            perPage: 1,\n          },\n        },\n      },\n      feedbacks\n    }\n  },\n  components: {\n    Splide,\n    SplideSlide\n  }\n}\n</script>\n<style lang=\"scss\">\n.sliderFeed {\n  width: 100%;\n  splide-slide .is-activeCard{\n    border: 1px solid black;\n  }\n}\n.splide__slide.is-active{\n  background: rgba(254,196,29,0.3);\n}\n.slide__content {\n  padding: 10px;\n  display: flex;\n  align-items: center;\n  flex-direction: column;\n  .slide__logo {\n    img {\n      border-radius: 50%;\n      width: 110px;\n      height: 110px;\n    }\n  }\n  .slide__title{\n    margin: 15px 0;\n  }\n  .slide__text{\n    text-align: center;\n    line-height: 20px;\n  }\n}\n@media screen and (max-width: 767px){\n  .splide__slide{\n    background: rgba(254,196,29,0.3);\n  }\n}\n</style>"],"sourceRoot":""}]}