cks from \"../components/sliderFeedbacks\"\n\nexport default {\n  name: 'Home',\n  components: {\n    sliderFeedBacks\n  }\n}\n</script>\n\n<style scoped lang=\"scss\">\n@import \"../assets/styles/index\";\n\n.our {\n  padding-top: 100px;\n  background-image: url(\"../assets/images/sectionONas.jpg\");\n  background-color: #f0f0f0;\n  background-size: 65% 100vh;\n  background-position: right;\n  background-repeat: no-repeat;\n  height: 100vh; //480px\n  display: flex;\n  justify-content: center;\n  box-sizing: border-box;\n\n  &__container {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n  }\n\n  &__logo {\n    font-family: arial;\n    font-size: 60px;\n    span {\n      font-size: 56px;\n    }\n  }\n\n  &__text {\n    margin-top: 20px;\n    font-size: 19px;\n    color: #8e8e8e;\n  }\n\n  &__button {\n    margin-top: 30px;\n\n    .our__link {\n      display: flex;\n      justify-content: center;\n      align-items: center;\n      width: 80px;\n      padding: 13px 20px;\n      border: 2px solid #5f5f5f;\n      text-transform: uppercase;\n      text-decoration: none;\n      color: black;\n      font-weight: bolder;\n    }\n  }\n}\n\n@media screen and (max-width: 1024px) {\n  .our {\n    position: relative;\n\n    &::after {\n      content: \"\";\n      position: absolute;\n      top: 0;\n      left: 0;\n      width: 100%;\n      height: 100%;\n      background-color: rgba(0, 0, 0, 0.5);\n      z-index: 2;\n    }\n\n    background-size: cover;\n\n    &__container {\n      align-items: flex-start;\n    }\n\n    .wrap__info {\n      z-index: 3;\n    }\n\n    &__logo {\n      z-index: 3;\n      color: #fff;\n      text-align: left;\n    }\n\n    &__text {\n      color: #fff;\n      z-index: 3;\n    }\n\n    &__button {\n      z-index: 3;\n\n      .our__link {\n        color: #fff;\n        border-color: #fec41d;\n\n      }\n    }\n  }\n}\n\n@media screen and (max-width: 640px) {\n  .our {\n    &__container {\n      align-items: center;\n    }\n\n    &__logo {\n      text-align: left;\n    }\n\n    &__button {\n      .our__link {\n        width: 85%;\n      }\n    }\n  }\n}\n\n/*end our section*/\n/*experience start*/\n.experience {\n  height: 600px;\n  background-color: #f0f0f0;\n  background-size: 35% 100%;\n  background-image: url(\"../assets/images/experience.jpeg\");\n  background-repeat: no-repeat;\n  display: flex;\n  justify-content: center;\n\n  box-sizing: border-box;\n\n  &__header {\n    text-align: center;\n  }\n\n  &__container {\n    display: flex;\n    flex-direction: column;\n    align-items: flex-end;\n    justify-content: space-around;\n  }\n\n  &__boxes {\n    display: flex;\n    flex-wrap: wrap;\n    max-width: 600px;\n    border: 5px solid white;\n\n    .box {\n      box-sizing: border-box;\n      width: 300px;\n      height: 200px;\n      display: flex;\n      align-items: center;\n      justify-content: center;\n      font-family: Popins;\n      flex-direction: column;\n      font-size: 25px;\n      font-weight: bold;\n      text-align: center;\n\n      .num {\n        font-size: 35px;\n      }\n\n      &-yellow {\n        background-color: #fec41d;\n        color: #fff;\n      }\n\n      &-dark {\n        background-color: #252527;\n        color: #fff;\n      }\n\n      &-gray {\n        background-color: #f0f0f0;\n      }\n\n      &-white {\n        background-color: #fff;\n      }\n    }\n\n  }\n\n\n}\n\n@media screen and (max-width: 1024px) {\n  .experience {\n    background: url(\"../assets/images/experienceLong.jpeg\");\n    background-size: cover;\n    background-position: center;\n\n    &__container {\n      align-items: center;\n\n    }\n  }\n}\n\n@media screen and (max-width: 640px) {\n  .experience {\n    height: 600px;\n    position: relative;\n\n    &::after {\n      content: \"\";\n      position: absolute;\n      top: 0;\n      left: 0;\n      width: 100%;\n      height: 100%;\n      background-color: rgba(0, 0, 0, 0.5);\n      z-index: 2;\n    }\n\n    &__container {\n      align-items: center;\n      z-index: 3;\n    }\n\n    &__header {\n      color: #fff;\n\n      margin-bottom: 0px;\n    }\n\n    &__boxes {\n      flex-direction: column;\n      border: none;\n      width: 90%;\n\n      .box {\n        width: 100%;\n        height: 130px;\n      }\n\n    }\n  }\n}\n\n/*end experience*/\n.slider{\n  display: flex;\n  justify-content: center;\n\n  &__container{\n    display: flex;\n    flex-direction: column;\n    box-sizing: border-box;\n  }\n  &__header{\n    margin-top: 0.5em;\n    letter-spacing: -1px;\n    align-self: center;\n    justify-self: center;\n  }\n}\n\n\n</style>\n"],"sourceRoot":""}]}